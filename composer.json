{
    "name": "storix/storix",
    "description": "ERP-grade Filament plugin for reusable container lifecycle and movement tracking.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "filament/filament": "^4.0",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "orchestra/testbench": "^10.0",
        "nunomaduro/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "rector/rector": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Storix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Storix\\Tests\\": "tests/",
            "Storix\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Storix\\StorixServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
